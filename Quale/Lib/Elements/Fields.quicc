element(BoxTextFieldView_Main)
    <textarea readonly type="text"
     {{#field.f.width}}cols="{{field.f.width}}"{{/field.f.width}}
     {{#field.f.y}}rows="{{field.f.y}}"{{/field.f.y}}{{^field.f.y}}rows="{{field.f.y}}"{{/field.f.y}}
    >[[[{{pop}}]]]</textarea>

