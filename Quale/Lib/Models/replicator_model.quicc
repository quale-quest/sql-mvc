
<#:model()

CREATE GENERATOR ZR$LOG_ID;
SET GENERATOR ZR$LOG_ID TO 1;
CREATE GENERATOR ZR$LAST_CURRENT_TRANSACTION;
SET GENERATOR ZR$LAST_CURRENT_TRANSACTION TO 0;	
CREATE GENERATOR ZR$MIN_CURRENT_TRANSACTION;
SET GENERATOR ZR$MIN_CURRENT_TRANSACTION TO 0;	



CREATE TABLE ZR$LOG
(
  TID Bigint,
  ID Bigint NOT NULL PRIMARY KEY,
  SHARD_ID  Varchar(32),
  TS Timestamp DEFAULT current_timestamp,  
  JSON Varchar(8000)
);

CREATE INDEX IDX_ZR$LOG ON ZR$LOG (SHARD_ID,TID,ID);


#>









